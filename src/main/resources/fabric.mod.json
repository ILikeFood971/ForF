{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "A Friend or Foe mod for you to play with your friends. Based on the YouTube Friend or Foe series.",
	"authors": [
		"ILikeFood971"
	],
	"contact": {
		"homepage": "https://github.com/ILikeFood971",
		"sources": "https://github.com/ILikeFood971/forf"
	},
	"license": "GPL-3.0",
	"icon": "assets/forf/icon.png",
	"environment": "server",
	"entrypoints": {
		"server": [
          "net.ilikefood971.forf.Forf"
		],
		"client": [
			"net.ilikefood971.ForfClient"
		]
	},
	"mixins": [
		"forf.mixins.json",
		{
			"config": "forf.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=${fabric_loader_version}",
		"minecraft": "${minecraft_dependency}",
		"java": ">=${java_version}",
		"fabric-api": ">=${fabric_api_version}"
	},
	"includes": {
		"sgui": ">=${sgui_version}",
		"server-translations": ">=${server_translations_version}",
		"polymer": ">=${polymer_core_version}"
	},
	"suggests": {
		"voicechat": ">=${voicechat_version}"
	}
}